{
  "name": "@josundt/file-system",
  "version": "1.0.0",
  "description": "Implementations of the whatwg File System standard with ponyfill support",
  "keywords": [
    "File System",
    "FileSystem",
    "WritableStream",
    "TransportStream",
    "FileSystemWritableFileStream",
    "showSaveFilePicker"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./downloader": {
      "types": "./dist/types/downloader/file-handle.d.ts",
      "default": "./dist/esm/downloader/file-handle.js"
    },
    "./show-save-file-picker": {
      "types": "./dist/types/show-save-file-picker.d.ts",
      "default": "./dist/esm/show-save-file-picker.js"
    }
  },
  "main": "index.js",
  "files": [
    "./dist",
    "./src"
  ],
  "scripts": {
    "compile": "npm run compile:ts",
    "compile:ts": "tsc -p ./tsconfig.json --noemit",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint src --format visualstudio --ext .ts,.tsx",
    "start": "webpack serve",
    "build": "rimraf ./dist/**/* && tsc -p ./tsconfig.dist.json && webpack --mode production",
    "test": "echo \"No test exists\"",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josundt/josundt-file-system.git"
  },
  "author": {
    "name": "JÃ¸rn Andre Sundt",
    "email": "spam@jasmin.no"
  },
  "license": "ISC",
  "devDependencies": {
    "@josundt/eslint-config": "4.7.3",
    "@josundt/tsconfig-rules": "4.7.3",
    "copy-webpack-plugin": "11.0.0",
    "html-webpack-plugin": "5.5.0",
    "resolve-typescript-plugin": "1.2.0",
    "rimraf": "3.0.2",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3"
  }
}
